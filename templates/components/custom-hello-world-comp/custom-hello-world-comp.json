{
  "$schema": "http://json-schema.org/schema#",
  "id": "custom-hello-world-comp",
  "name": "custom-hello-world-comp",
  "description": "custom hello world component",
  "type": "object",
  "category": "Custom",
  "properties": {
    "id": {
      "allOf": [
        {
          "$ref": "../definitions/valid-component-id.json"
        },
        {
          "type": "string",
          "title": "Id",
          "description": "The Id of the component",
          "default": "",
          "minLength": 1
        }
      ],
      "order": 1
    },
    "greeting": {
      "type": "string",
      "title": "Greeting",
      "description": "The string prepended to world",
      "default": "Salutations",
      "order": 2
    }
  },
  "required": [
    "id"
  ]
}